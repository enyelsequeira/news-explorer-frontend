/src/components/SearchForm/SearchForm.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/Footer/Footer.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/Main/Main.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/Header/Header.js",["122"],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/About/About.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/contexts/CurrentUserContext.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/SavedNews/SaveNews.js",["123","124","125"],"/* eslint-disable no-unused-vars */\n/* eslint-disable react/destructuring-assignment */\n/* eslint-disable no-plusplus */\nimport React, { useContext, useEffect, useState } from 'react';\nimport NewsCardList from '../NewsCardList/NewsCardList';\nimport SavedHeader from '../Header/SavedHeader';\nimport CurrentUserContext from '../../contexts/CurrentUserContext';\nimport MainBackend from '../../utils/practicum-api';\n\nconst SaveNews = (props) => {\n  const currentUser = useContext(CurrentUserContext);\n  const userToken = localStorage.getItem('jwt');\n  const [savedCards, setSavedCards] = useState([]);\n  const savedLength = savedCards.length;\n\n  const mainApi = new MainBackend({\n    baseUrl: 'http://localhost:3000',\n    headers: {\n      // Accept: 'application/json',\n      'content-type': 'application/json',\n      Authorization: `Bearer ${userToken}`,\n    },\n  });\n\n  const fetchCards = () => {\n    mainApi.getSavedArticles().then((res) => {\n      console.log(res, 'coming from 12345');\n      setSavedCards(res.articles);\n    }).catch((err) => console.log(err));\n  };\n\n  useEffect(() => {\n    fetchCards();\n  }, []);\n\n  const byKeywords = () => {\n    const keywords = [];\n    savedCards.forEach((card) => {\n      keywords.push(`${card.keyword} `);\n    });\n\n    const sortByWord = (arr) => {\n      const frequency = {};\n\n      arr.forEach((value) => { frequency[value] = 0; });\n      const uniques = arr.filter((value) => ++frequency[value] === 1);\n      return uniques.sort((a, b) => frequency[b] - frequency[a]);\n    };\n\n    const keywordsList = sortByWord(keywords);\n    const keywordsDisplay = keywordsList.join(', ');\n\n    if (keywords.length <= 3) {\n      return keywordsDisplay;\n    }\n    const keywordCut = keywordsList.slice(0, 2);\n    const display = `${keywordCut.join(', ')}, and ${keywordsList.length - 2} others`;\n    return display;\n\n    // return keywords.slice(0, 3); // dynamic to only show +2\n  };\n\n  const handleChange = (cardId) => {\n    console.log(savedCards, 'this is my other id?');\n    const newCards = savedCards.filter((c) => c.id !== cardId);\n    setSavedCards(newCards);\n    fetchCards();\n  };\n  return (\n    <section className=\"saved\">\n      <SavedHeader buttonClick={props.headerClick} />\n      <p className=\"saved__title\">Saved Articles</p>\n      <h1 className=\"saved__heading\">\n        {currentUser.name}\n        , You have\n        {' '}\n        {savedLength}\n        {' '}\n        saved Articles\n      </h1>\n      <p className=\"saved__text\">\n        By Keywords:\n        <span className=\"saved__keywords\">\n          {byKeywords()}\n        </span>\n      </p>\n      <NewsCardList onChange={handleChange} cards={savedCards} savedArticles=\"true\" loggedIn=\"true\" hover=\"Remove from saved\" />\n\n    </section>\n  );\n};\n\nexport default SaveNews;\n","/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/NewsCardList/NewsCardList.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/Card/Card.js",["126","127"],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/Header/SavedHeader.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/PopupWihForm/RegistrationSuccessful.js",[],"/home/enyel/Desktop/yandex1/news-explorer-frontend/src/components/PopupWihForm/Form/SingInPopout.js",[],"/home/enyel/Desk